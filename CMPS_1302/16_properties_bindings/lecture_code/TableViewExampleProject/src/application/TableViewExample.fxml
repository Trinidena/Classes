<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TableViewExampleController">
   <children>
      <TableView fx:id="tableView" layoutX="33.0" layoutY="89.0" prefHeight="283.0" prefWidth="345.0">
        <columns>
          <TableColumn fx:id="wordColumn" editable="false" prefWidth="173.0" text="Words" />
            <TableColumn fx:id="numberColumn" prefWidth="171.0" text="Number" />
        </columns>
      </TableView>
      <TextField fx:id="wordField" layoutX="33.0" layoutY="30.0" onAction="#handleTextField" />
      <TextField fx:id="numberField" layoutX="219.0" layoutY="30.0" onAction="#handleTextField" />
   </children>
</AnchorPane>